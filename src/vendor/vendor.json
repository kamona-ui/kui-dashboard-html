{
  "vendors": {
    "alpinejs": {
      "js": {
        "from": "./node_modules/alpinejs/dist/cdn.min.js",
        "to": "./public/assets/js/vendor/alpinejs.min.js"
      }
    },
    "@alpinejs/collapse": {
      "js": {
        "from": "./node_modules/@alpinejs/collapse/dist/cdn.min.js",
        "to": "./public/assets/js/vendor/alpinejs-collapse.min.js"
      }
    },
    "perfect-scrollbar": {
      "js": {
        "from": "./node_modules/perfect-scrollbar/dist/perfect-scrollbar.min.js",
        "to": "./public/assets/js/vendor/perfect-scrollbar.min.js"
      },
      "css": {
        "from": "./node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
        "to": "./public/assets/css/vendor/perfect-scrollbar.css"
      }
    }
  }
}
